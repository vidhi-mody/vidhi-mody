</a>

<!-- SOCIAL -->
<h3>:milky_way: Find me in the 'verse</h3>

<p>
    <% for(const { name, url, logo } of social) { %><a href="<%= url %>" target="_blank">
        <img alt="<%= name %>" src="<%= logo %>" />
    </a><% } %>
</p>

<hr />

