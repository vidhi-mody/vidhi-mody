<h1>Hey there! <img src="https://emojis.slackmojis.com/emojis/images/1531849430/4246/blob-sunglasses.gif?1531849430" width="30"/></h1>

<p align="center">Welcome to my page! </br> I'm <%= meta.firstName %>, a <%= meta.role %> from <img src="<%= meta.location.logo %>" width="13"/> <b><%= meta.location.city %>, <%= meta.location.country %></b>.</p>

<p>
    <%= meta.about %>
</p>

<div align="center">
    <img src="https://cdn.dribbble.com/users/1314475/screenshots/3031368/me.gif" alt="hey" />
</div>

<!-- TECHNOLOGIES -->
<h3>:zap: Technologies</h3>

<p>
<% for(const [name, logo] of Object.entries(technologies)) { %><img alt="<%= name %>" src="<%= logo %>" /> <% } %>
</p>

<!-- PROJECTS -->
<h3>:computer: Open Source Projects</h3>

<table>
    <thead>
        <td>🎁 Project </td>
        <td> ⭐ Stars </td>
        <td> 📚 Forks </td>
    </thead>
    <% for(const project of projects) { %><tr>
        <td><a href="<%= project.url %>"><%= project.name %></a></td>
        <td><img src="<%= project.stars %>" /></td>
        <td><img src="<%= project.forks %>" /></td>
    </tr><% } %>
</table>

<!-- POSTS -->
<h3>:mailbox_with_mail: Blogs</h3>

<ul>
    <% for(const post of posts) { %><li>
        <a href="<%= post.url %>"><b><%= post.title %></b></a>
    </li><% } %>
</ul>

<!--START_SECTION:waka-->
<!--END_SECTION:waka-->
<p>
<img alt="Visitors" src="https://visitor-badge.laobi.icu/badge?page_id=vidhi-mody&color=blue"/>
<img alt="Profile Views" src="https://komarev.com/ghpvc/?username=vidhi-mody"/>
</p>

